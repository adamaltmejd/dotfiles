      accentColor:
        red: 246
        green: 243
        blue: 237
        alpha: 1
    fonts: {}
    tabs:
      compactTabs: true
      tintedTabBar: true
    treeView:
      blendTabs: true
      compactList: true
    ui:
      animations: false
      panelContrast: true
  "autocomplete-bibtex":
