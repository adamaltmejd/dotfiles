    backUpBeforeSaving: true
    fontFamily: "Ubuntu Mono"
    fontSize: 13
    invisibles: {}
    scrollPastEnd: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 4
