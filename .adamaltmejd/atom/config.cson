    enablePandoc: true
    pandocArguments: [
      "--smart"
    ]
    pandocBIBFileFallback: "/Users/ad.3103/Dropbox/Reading/Library/library.bib"
    pandocBibliography: true
    pandocCSLFileFallback: "/Users/ad.3103/.adamaltmejd/pandoc/chicago-author-date-adam.csl"
